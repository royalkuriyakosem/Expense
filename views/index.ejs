<!DOCTYPE html>
<html lang="en">
<head>
    <title>Expense Tracker</title>
</head>
<body>
    <h1>Expense Tracker</h1>
    <a href="/logout">Logout</a>
    <a href="/add-expense">Add Expense</a>
    <ul>
        <% expenses.forEach(expense => { %>
            <li><%= expense.description %> - $<%= expense.amount %> - <%= expense.date %></li>
        <% }); %>
    </ul>
</body>
</html>

